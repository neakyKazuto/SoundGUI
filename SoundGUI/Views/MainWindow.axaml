<Window
    Icon="/Assets/avalonia-logo.ico"
    Title="SoundGUI"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="SoundGUI.Views.MainWindow"
    x:DataType="vm:MainWindowViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:SoundGUI.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <Grid>
        <TabControl>

            <TabItem Header="Main">
                <TabItem.Content>
                    <ContentControl Content="{Binding MainVMD}" />
                </TabItem.Content>
            </TabItem>

            <TabItem Header="Settings">
                <TabItem.Content>
                    <ContentControl Content="{Binding SettingsVMD}" />
                </TabItem.Content>
            </TabItem>
        </TabControl>
    </Grid>

</Window>
